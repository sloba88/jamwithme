                    {% if is_mobile() %}
                        <li class="shouts {% if app.request.attributes.get('_route') == 'shouts' %}active{% endif %}"><a href="{{ path('shouts') }}">{{ 'text.shouts'|trans }}</a></li>
                    {% endif %}
                    <li class="profile {% if app.request.attributes.get('_route') == 'fos_user_profile_show' or ( app.request.attributes.get('_route') == 'musician_profile' and app.user.id == user.id ) %}active{% endif %}"><a href="{{ path('musician_profile', {'username': app.user.username}) }}">{{ 'text.profile'|trans }}</a></li>
                    <li class="messages {% if app.request.attributes.get('_route') == 'inbox' %}active{% endif %}"><a href="{{ path('inbox') }}" class="inbox"><span class="badge"></span>{{ 'text.messages'|trans }}</a></li>
                    <li class="settings last {% if app.request.attributes.get('_route') == 'fos_user_profile_edit' %}active{% endif %}"><a href="{{ path('fos_user_profile_edit') }}">{{ 'text.settings'|trans }}</a></li>
                </ul>
            </div><!--sidebar-nav ends-->

            <div class="sidebar-feedback">
                <p>We <i class="fa fa-heart-o"></i> feedback</p>
                <ul>
                    <li><a href="https://www.facebook.com/jamifind"><i class="fa fa-facebook"></i></a></li>
                    <li><a href="https://twitter.com/Jamifind"><i class="fa fa-twitter"></i></a></li>
                    <li><a href="mailto:info@jamifind.com?subject=Feedback on Jamifind"><i class="fa fa-envelope"></i></a></li>
                </ul>
            </div>

            <div class="sidebar-logout">
                <ul class="sidebar-list">
                    <li class="logout"><a href="{{ path('fos_user_security_logout') }}">{{ 'text.logout'|trans }}</a></li>
                </ul>
            </div>

        </div>
    </aside><!--sidebar ends-->

{% endif %}