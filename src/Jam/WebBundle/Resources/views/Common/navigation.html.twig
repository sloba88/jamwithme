{% if is_granted("ROLE_USER") %}

    <aside class="col-sm-2 sidebar">
        <div class="sidebar-inner">

            <div class="profile-info">
                <a href="{{ path('musician_profile', {'username': app.user.username}) }}">
                    <img class="user-image" src="{{ app.user.avatar | imagine_filter('my_medium_1') }}" alt="Profile Photo" />
                </a>
                <h3 class="user-name">{{ app.user.username }}</h3>
                <a href="{{ path('fos_user_profile_edit') }}" class="link">{{ 'text.edit.profile'|trans }}</a>
            </div><!--profile-info begins-->

            <div class="sidebar-nav">
                <ul class="sidebar-list">
                    <li class="dashboard {% if app.request.attributes.get('_route') == 'home' %}active{% endif %}"><a href="{{ path('home') }}">{{ 'text.musicians'|trans }}</a></li>
